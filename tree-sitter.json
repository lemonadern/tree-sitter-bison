{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bison",
      "camelcase": "Bison",
      "title": "Bison",
      "scope": "source.bison",
      "file-types": [
        "bison"
      ],
      "injection-regex": "^bison$",
      "class-name": "TreeSitterBison"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Bison grammar for tree-sitter",
    "authors": [
      {
        "name": "lemonadern"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-bison"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}